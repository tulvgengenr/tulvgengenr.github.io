---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
---

<BaseLayout title = "Projects">
  <div>
    <div class="text-3xl w-full font-bold mb-5">参与的项目</div>
  </div>

  <HorizontalCard
    title="SCOW"
    img="/scow.svg"
    desc="SCOW是一个面向超算使用员和管理员的、基于Web的超算门户(portal)和管理系统(mis)。本人主要负责跨集群和跨中心文件传输模块的设计、实现和优化。这也是我的本科毕业设计。"
    url="https://github.com/PKUHPC/SCOW"
    badge="项目成员"
  />
  <div class="divider my-0"></div>

  <div>
    <div class="text-3xl w-full font-bold mb-5">个人的项目</div>
  </div>

</BaseLayout>
